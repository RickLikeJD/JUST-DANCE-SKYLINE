	"ban-player": {
        "title": "Ban player",
        "parameters": [{
                "id": "banningmethod",
                "title": "Banning method",
                "type": "string",
                "control": "select",
                "value": "nameOnPlatform",
                "options": [{
                        "value": "nameOnPlatform",
                        "name": "nameOnPlatform"
                    },
					{
                        "value": "profileId",
                        "name": "profileId"
                    },
					{
                        "value": "userId",
                        "name": "userId"
                    }
                ]
            }, {
                "id": "banvalue",
                "title": "Banning method value",
                "type": "string",
                "control": "input"
            }, {
                "id": "platformType",
                "title": "Platform type ~(for nameOnPlatform)~",
                "type": "string",
                "control": "select",
                "value": "uplay",
                "options": [{
                        "value": "uplay",
                        "name": "uplay"
                    },
					{
                        "value": "switch",
                        "name": "switch"
                    },
					{
                        "value": "psn",
                        "name": "psn"
                    },
					{
                        "value": "wiiu",
                        "name": "wiiu"
                    },
					{
                        "value": "all",
                        "name": "all"
                    }
                ]
            }, {
                "id": "bannedBy",
                "title": "Banned by",
                "type": "string",
                "control": "select",
                "options": [{
                        "value": "yunyl",
                        "name": "yunyl"
                    },
					{
                        "value": "dev",
                        "name": "dev"
                    }
                ]
            }, {
                "id": "reason",
                "title": "Ban reason",
                "type": "string",
                "control": "input",
                "placeholder": "If empty, \"No reason given\" will be default reason."
            }
        ]
    },
