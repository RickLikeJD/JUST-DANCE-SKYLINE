	"request": {
        "title": "Request",
        "parameters": [{
                "id": "ticketType",
                "title": "Ticket type",
                "type": "string",
                "control": "select",
                "value": "player",
                "options": [{
                        "value": "dev",
                        "name": "Developer"
                    }
                ]
            }, {
                "id": "profileId",
                "title": "Profile id",
                "type": "uuid4",
                "control": "input",
                "optional": true,
                "placeholder": "If empty, will be randomly generated"
            }, {
                "id": "language",
                "title": "Language",
                "type": "string",
                "control": "language",
                "optional": true
            }, {
