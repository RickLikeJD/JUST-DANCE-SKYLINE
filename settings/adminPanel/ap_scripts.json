{
    "add-songdb": {
        "title": "Add map to songDB",
        "parameters": [{
                "id": "mapName",
                "title": "Map name",
                "type": "string",
                "control": "input"
            }, {
                "id": "mapType",
                "title": "Map type",
                "type": "string",
                "control": "select",
                "value": "custom",
                "options": [{
                        "value": "custom",
                        "name": "custom"
                    }, {
                        "value": "jdu",
                        "name": "jdu"
                    }
                ]
            }, {
                "id": "artist",
                "title": "Artist",
                "type": "string",
                "control": "input"
            }, {
                "id": "title",
                "title": "Title",
                "type": "string",
                "control": "input"
            }, {
                "id": "coachCount",
                "title": "coachCount",
                "type": "int",
                "control": "number",
                "min": 1,
                "max": 4,
                "value": 1
            }, {
                "id": "beatsArray",
                "title": "Markers array",
                "type": "string",
                "control": "input",
				"placeholder": "(for audioPreview)"
            }, {
                "id": "endbeat",
                "title": "Endbeat",
                "type": "string",
                "control": "input",
				"placeholder": "(required for WDF)"
            }, {
                "id": "platform",
                "title": "Platform",
                "type": "string",
                "control": "select",
                "value": "pc",
                "options": [{
                        "value": "pc",
                        "name": "PC"
                    }, {
                        "value": "ps4",
                        "name": "PS4"
                    }, {
                        "value": "nx",
                        "name": "NX"
                    }
                ]
            }, {
                "id": "difficulty",
                "title": "Difficulty",
                "type": "int",
                "control": "number",
                "min": 1,
                "max": 4,
                "value": 1
            }, {
                "id": "lyricscolor",
                "title": "lyricsColor",
                "type": "string",
                "control": "input",
				"placeholder": "FF00000"
            }, {
                "id": "originaljdversion",
                "title": "JD Version",
                "type": "int",
                "control": "select",
                "value": 1,
                "options": [{
                        "value": 1,
                        "name": "Just Dance 1"
                    }, {
                        "value": 2,
                        "name": "Just Dance 2"
                    }, {
                        "value": 3,
                        "name": "Just Dance 3"
                    }, {
                        "value": 4,
                        "name": "Just Dance 4"
                    }, {
                        "value": 2014,
                        "name": "Just Dance 2014"
                    }, {
                        "value": 2015,
                        "name": "Just Dance 2015"
                    }, {
                        "value": 2016,
                        "name": "Just Dance 2016"
                    }, {
                        "value": 2017,
                        "name": "Just Dance 2017"
                    }, {
                        "value": 2018,
                        "name": "Just Dance 2018"
                    }, {
                        "value": 2019,
                        "name": "Just Dance 2019"
                    }, {
                        "value": 2020,
                        "name": "Just Dance 2020"
                    }, {
                        "value": 2021,
                        "name": "Just Dance 2021"
                    }, {
                        "value": 4514,
                        "name": "Just Dance Asia (China, Japan, Korea)"
                    }
                ]
            }, {
                "id": "customtype",
                "title": "CustomType",
                "type": "string",
                "control": "input",
				"placeholder": "(Leave this empty if the song you are adding is not an ALTERNATE)"
            }, {
                "id": "skumd5",
                "title": "MD5 of the SKU ZIP",
                "type": "string",
                "control": "input"
            }, {
                "id": "onea",
                "title": "SongColor 1A",
                "type": "string",
                "control": "input",
                "placeholder": "(Leave all color boxes empty to use all default colors)"
            }, {
                "id": "oneb",
                "title": "SongColor 1B",
                "type": "string",
                "control": "input",
				"placeholder": "FF00000"
            }, {
                "id": "twoa",
                "title": "SongColor 2A",
                "type": "string",
                "control": "input",
				"placeholder": "FF00000"
            }, {
                "id": "twob",
                "title": "SongColor 2B",
                "type": "string",
                "control": "input",
				"placeholder": "FF00000"
            }
        ]
    },
